{
    "name": "@chip-in-v2/docusaurus-root",
    "version": "1.0.0",
    "description": "Document root for Chip-in",
    "repository": "https://github.com/nec-dis/document-root",
    "author": "Mitsuru Nakakawaji <mitsuru@procube.jp>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "clear-doc": "chip-in-docusaurus clear && rm -rf .chip-in-docusaurus",
